# backend environment variables

# Google Maps backend key (Routes, Geocoding, etc.)
GOOGLE_MAPS_BACKEND_KEY=your_google_maps_backend_key_here

# U.S. Energy Information Administration API key (fuel prices)
EIA_API_KEY=your_eia_api_key_here

# CORS origin (frontend URL allowed to call backend)
# for local dev: http://localhost:5173
# for production: your deployed Vercel frontend URL
CORS_ORIGIN=http://localhost:5173

# Render will auto-assign PORT, so donâ€™t hardcode in production
PORT=3000
